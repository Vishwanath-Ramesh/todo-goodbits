{
  "name": "react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A React boilerplate setup with webpack and babel setup",
  "main": "index.js",
  "scripts": {
    "babel": "babel src --out-dir lib",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --ignore-path .gitignore --write .",
    "check-format": "prettier --ignore-path .gitignore --list-different .",
    "validate": "yarn lint && yarn test && yarn build",
    "validate-parallel": "npm-run-all --parallel format lint test build",
    "test": "jest",
    "test:related": "jest --findRelatedTests",
    "test:report": "jest --coverage && serve ./coverage/lcov-report",
    "test:watch": "jest --watchAll",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "setup": "yarn install && yarn validate",
    "build": "webpack --env NODE_ENV=production",
    "dev": "webpack serve --env NODE_ENV=development",
    "docker-build": "docker build -t react-boilerplate .",
    "docker-run": "docker run react-boilerplate",
    "docker-start": "docker start react-boilerplate",
    "docker-stop": "docker stop react-boilerplate",
    "docker-clean-containers": "docker rm $(docker ps -aq)",
    "docker-clean-images": "docker rmi $(docker images -aq)",
    "docker-clean": "yarn docker-clean-containers && yarn docker-clean-images",
    "docker-enter": "docker exec -ti react-boilerplate bash"
  },
  "keywords": [],
  "author": "Vishwanath R",
  "license": "ISC",
  "dependencies": {
    "@date-io/moment": "^2.13.1",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "@mui/styled-engine-sc": "^5.4.2",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "axios": "^0.21.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.2",
    "reselect": "^4.1.5",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "styled-components": "^5.3.3",
    "swiper": "^8.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/dom": "7.31.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^13.1.9",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "lint-staged": "^10.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^5.3.0",
    "test-data-bot": "^0.8.0",
    "typescript": "^4.5.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.+(js|jsx|ts|tsx|json|css)": [
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vishwanath-Ramesh/react-boilerplate.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}